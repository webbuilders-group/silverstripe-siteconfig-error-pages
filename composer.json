{
    "name": "webbuilders-group/silverstripe-siteconfig-error-pages",
    "description": "Moves the SilverStripe Error Page management from the page tree to Settings section.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "siteconfig", "error pages", "management"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ed Chipman",
            "homepage": "https://webbuildersgroup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/recipe-cms": "^6",
        "webbuilders-group/silverstripe-gridfield-deleted-items": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~11.3"
    },
    "support": {
        "issues": "https://github.com/webbuilders-group/silverstripe-siteconfig-error-pages/issues"
    },
    "extra": {
        "expose": [
            "css",
            "javascript"
        ]
    },
    "autoload": {
        "psr-4": {
            "WebbuildersGroup\\SiteConfigErrorPages\\": "src/",
            "WebbuildersGroup\\SiteConfigErrorPages\\Tests\\": "tests/"
        }
    }
}
